{
  "name": "tarojs-table",
  "version": "1.1.5",
  "private": false,
  "description": "tarojs-table，一个简单的基础表格组件",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "tarojs",
    "tarojs-table"
  ],
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "less"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chutaozh/tarojs-table.git"
  },
  "author": "chutao.zhang@outlook.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chutaozh/tarojs-table/issues"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "rm": "node fs.config.js rm",
    "cp": "node fs.config.js cp",
    "tsc": "npm run rm && npm run cp && tsc --project tsconfig.build.json"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "peerDependencies": {
    "@tarojs/components": ">=3.6.0",
